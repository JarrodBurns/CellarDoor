[loggers]
keys=root


[handlers]
keys=fileHandler


[formatters]
keys=logFormatter


[logger_root]
level=INFO
handlers=fileHandler


[handler_fileHandler]
class=FileHandler
level=INFO
formatter=logFormatter
args=('placeholder.log', 'a')


[formatter_logFormatter]
format=%(asctime)s | %(levelname)-8s | %(lineno)04d | %(filename)s    %(message)s

